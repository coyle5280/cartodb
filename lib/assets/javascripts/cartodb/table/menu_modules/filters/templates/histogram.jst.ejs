<div class="ui-datepicker-div"></div>
<a href="#remove" class="remove"></a>

<div class="header">
  <div class="title">
    <a class="toggleButton" style="float:left" href="#">Toggle </a>
    <strong class="legend">
      <% if (alias) { %>  
        <%- alias %>:
      <% } else { %>
        <%- legend %>:
      <% } %>
    </strong>
    <p class='range-text'></p>
    <div class="loading">Loading</div>
    <div class="empty">This column is empty</div>
    <div class="only">This column has one unique value</div>

    <div class="range-number">
        <input type="number" class="min" /> - <input type="number" class="max" />
        <br />
        <span class="subtext">Hit 'Enter' to save...</span>
    </div>
    <div class="range-date-text"></div>
    <div class="range">
      <input type="text" id:'calmin' class="date-min" readonly=true /> - <input type="text" id:'calmax' class="date-max" readonly=true /><a href="#" class="ui-close-calendar">Close</a>
    </div>
  </div>
</div>
<div class="hist <%- cid %>"></div>
